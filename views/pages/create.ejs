
<%- include('../partials/head.ejs') %>
<%- include('../partials/header.ejs') %>

<body class="text-center">
    
    
    <div class="g-3 container w-25">
        
        <form action="/product/create" method="POST">   
        <h3 class="mb-3">Create A Product</h3>
        
        <div class="form-floating mb-3">
            <input name="name" type="text" class="form-control" id="floatingName" placeholder="">
            <label for="floatingName" style="background-color: transparent;">Name</label>
        </div>
        
        
        <div class="form-floating mb-3">
            <input name="price" type="number" class="form-control" id="floatingPrice" placeholder="">
            <label for="floatingPrice" style="background-color: transparent;">Price</label>
        </div>
        
        
            
            <button class="btn btn-primary mb-3 mt-3" type="submit">Submit</button>
        </form>
        
    </div>
    








</body>

