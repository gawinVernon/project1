  <body class="text-center" id="body">

    
<main class="form-signin">
  <form action="/user/login" method="POST">
    <!-- <img class="mb-4" src="/docs/5.0/assets/brand/bootstrap-logo.svg" alt="" width="72" height="57"> -->
    <h1 class="h3 mb-3 fw-normal">Sign in</h1>


    <% if(failed.length > 0) {%>
      <p class="alert alert-danger"><%= failed %></p>
    <% } %>


    <div class="form-floating">
      <input type="email" name="email" class="form-control" id="floatingInput" placeholder="name@example.com" required="true">
      <label for="floatingInput" style="background-color: transparent;">Email address</label>
    </div>
    <div class="form-floating">
      <input type="password" name="password" class="form-control" id="floatingPassword" placeholder="Password" required="true">
      <label for="floatingPassword" style="background-color: transparent;">Password</label>
    </div>

    <button class="w-100 btn btn-lg btn-primary" type="submit">Sign in</button>
    <p class="mt-5 mb-3 text-muted">&copy; 2023 Atat Company</p>
  </form>
</main>


    
